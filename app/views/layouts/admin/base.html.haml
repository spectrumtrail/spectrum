!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %meta{:charset => "utf-8"}/
    %meta{:content => "width=device-width, initial-scale=1.0, shrink-to-fit=no", :name => "viewport"}/
    %meta{:content => "This is the admin side of Spectrum Trail Racing.", :name => "description"}/
    %meta{:content => "Spectrum Trail Racing", :name => "author"}/
    %link{:href => "https://cdn.jsdelivr.net/npm/froala-editor@2.9.1/css/froala_editor.pkgd.min.css", :rel => "stylesheet", :type => "text/css"}/
    %link{:href => "https://cdn.jsdelivr.net/npm/froala-editor@2.9.1/css/froala_style.min.css", :rel => "stylesheet", :type => "text/css"}/
    %title= "ADMIN #{controller_name.titleize} #{action_name.titleize}"
    %link{:href => "https://fonts.googleapis.com/css?family=Nanum+Pen+Script|Nunito:400,600,700,800|Roboto:400,500,700&display=swap", :rel => "stylesheet"}/
    = csrf_meta_tag
    = favicon_link_tag asset_path("favicon")
    = stylesheet_link_tag "application", media: "all"
    = javascript_include_tag "application"
    %script{:src => "https://cdn.jsdelivr.net/npm/froala-editor@2.9.1/js/froala_editor.pkgd.min.js", :type => "text/javascript"}
  %body
    = render "admin/navigation/sidebar"
    %section.content
      = render "admin/navigation/header"
      = render partial: 'flash_messages', flash: flash
      .content-inner.content-docs
        .container-fluid
          = yield
      = render "admin/navigation/footer"
